{
  "name": "dinsro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "sc": "shadow-cljs",
    "start": "shadow-cljs watch app",
    "build": "yarn clean && shadow-cljs release app",
    "test": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "test:once": "shadow-cljs compile test && node out/test.js",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "rimraf public/js"
  },
  "devDependencies": {
    "clj-kondo": "^2020.1.13",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-cljs-test": "^0.1.0",
    "semantic-ui-react": "^2.0.1",
    "shadow-cljs": "^2.11.5"
  },
  "dependencies": {
    "@cljs-oss/module-deps": "^1.1.1",
    "@js-joda/core": "^3.2.0",
    "create-react-class": "^15.7.0",
    "highcharts": "^8.2.2",
    "highlight.js": "9.18.1",
    "hljs": "^6.2.3",
    "jquery": "^3.5.1",
    "marked": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-layout": "0.16.6",
    "react-highlight.js": "1.0.7",
    "react-icons": "2.2.7",
    "semantic-ui-css": "^2.4.1",
    "shadow": "0.0.1",
    "xmlhttprequest": "^1.8.0"
  }
}
